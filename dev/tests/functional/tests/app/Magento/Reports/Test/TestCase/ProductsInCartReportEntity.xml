<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../etc/global/variations.xsd">
    <testCase name="ProductsInCartReportEntity">
        <variation name="ProductsInCartReportEntityVariation1">
            <data name="product/dataSet">default</data>
            <data name="carts">1</data>
            <data name="isGuest">0</data>
            <data name="constraint">assertProductInCartResult</data>
        </variation>
        <variation name="ProductsInCartReportEntityVariation2">
            <data name="product/dataSet">default</data>
            <data name="carts">2</data>
            <data name="isGuest">1</data>
            <data name="constraint">assertProductInCartResult</data>
        </variation>
    </testCase>
</config>
