<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../etc/global/variations.xsd">
    <testCase name="UserLoginAfterChangingPermissionsTest">
        <variation name="UserLoginAfterChangingPermissionsTestVariation1">
            <data name="role/data/rolename">AdminRole%isolation%</data>
            <data name="role/data/resource_access">Custom</data>
            <data name="role/data/roles_resources">Sales</data>
            <data name="updatedRole/data/rolename">UpdatedAdminRole%isolation%</data>
            <data name="updatedRole/data/resource_access">All</data>
            <data name="constraint">assertUserSuccessLogin</data>
        </variation>
    </testCase>
</config>
