<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../etc/global/variations.xsd">
    <testCase name="SuggestSearchingResultEntityTest">
        <variation name="SuggestSearchingResultEntityTestVariation1">
            <data name="catalogSearch/data/query_text/value">catalogProductSimple::name</data>
            <data name="catalogSearch/data/num_results">-</data>
            <data name="constraint">assertSuggestSearchingResult</data>
        </variation>
        <variation name="SuggestSearchingResultEntityTestVariation2">
            <data name="catalogSearch/data/query_text/value">catalogProductSimple::sku</data>
            <data name="catalogSearch/data/num_results">1</data>
            <data name="constraint">assertSuggestSearchingResult</data>
        </variation>
    </testCase>
</config>
