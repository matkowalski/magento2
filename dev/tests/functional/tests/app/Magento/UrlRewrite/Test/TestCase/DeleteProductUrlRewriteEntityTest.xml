<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../etc/global/variations.xsd">
    <testCase name="DeleteProductUrlRewriteEntityTest">
        <variation name="DeleteProductUrlRewriteEntityTestVariation1">
            <data name="productRedirect/dataSet">default_without_target</data>
            <data name="productRedirect/data/target_path/entity">product/%catalogProductSimple::product_100_dollar%</data>
            <data name="constraint">assertUrlRewriteDeletedMessage, assertUrlRewriteNotInGrid, assertPageByUrlRewriteIsNotFound</data>
        </variation>
    </testCase>
</config>
